import java.util.Scanner;
import java.io.*;
class Main {
    public static void main(String[] args) {
		int n1,n2,n3;
		Scanner sc = new Scanner(System.in);
		n1=sc.nextInt();
		n2=sc.nextInt();
		n3=sc.nextInt();
		if(n1>n2 && n1>n3){
			System.out.println("Maximum among all three numbers is: "+n1);
		}
		else if(n2>n1 && n2>n3){
			System.out.println("Maximum among all three numbers is: "+n2);
		}
		else{
			System.out.println("Maximum among all three numbers is: "+n3);
		}
    }
}
